{
  "expo": {
    "newArchEnabled": true,
    "name": "Audiomix",
    "slug": "myApp",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/AudioMixlogo-01.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AudioMixlogo-01.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/AudioMixlogo-01.jpg",
        "backgroundColor": "#000000"
      },
      "package": "com.audiomix.myapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/AudioMixlogo-01.jpg"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-font"
    ]
  }
}
